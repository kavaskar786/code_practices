digraph G {
	A [label=Alumni shape=actor]
	T [label=Teacher shape=actor]
	S [label=Student shape=actor]
	Admin [label=Administrator shape=actor]
	Reg [label="Register/Login"]
	Profile [label="Manage Profile"]
	Search [label="Search Directory"]
	Msg [label="Send Message"]
	Forum [label="Participate in Forum"]
	Event [label="Manage Events"]
	Job [label="Job Board"]
	Mentor [label="Mentorship Program"]
	News [label="Read News"]
	Upload [label="Upload Media"]
	Analytics [label="View Analytics"]
	A -> Reg
	A -> Profile
	A -> Search
	A -> Msg
	A -> Forum
	A -> Event
	A -> Job
	A -> Mentor
	A -> News
	A -> Upload
	T -> Reg
	T -> Profile
	T -> Search
	T -> Msg
	T -> Forum
	T -> Event
	T -> Mentor
	T -> News
	T -> Upload
	S -> Reg
	S -> Profile
	S -> Search
	S -> Msg
	S -> Forum
	S -> Event
	S -> Job
	S -> Mentor
	S -> News
	S -> Upload
	Admin -> Event
	Admin -> News
	Admin -> Upload
	Admin -> Analytics
}
